#!/usr/bin/make -f

override_dh_clean:
	rm -rf build/sphinx
	dh_clean

override_dh_auto_install:
	dh_auto_install -- --install-html=$(CURDIR)/debian/python-pytango/usr/share/doc/python-pytango/html

override_dh_auto_build:
	dh_auto_build -- --without-spock

%:
	dh $@ --with python2 --with sphinxdoc
